const tokenModels = [
  {
    name: 'Ethereum (ETH)',
    model: 'Gas / Network Fee',
    lesson: 'Must hold ETH to use network = genuine utility demand. But gas costs kill micropayments.',
    outcome: 'üü¢ Success',
  },
  {
    name: 'Bittensor (TAO)',
    model: 'Competitive AI Quality',
    lesson: 'Miners compete on output quality. Right idea, but incentive gaming is rampant in practice.',
    outcome: 'üü° Partial',
  },
  {
    name: 'Helium (HNT)',
    model: 'Earn by Contributing',
    lesson: 'Brilliant incentive alignment initially, collapsed when real usage couldn\'t justify token price.',
    outcome: 'üî¥ Cautionary',
  },
  {
    name: 'Virtuals (VIRTUAL)',
    model: 'Launchpad Tax',
    lesson: 'Taxing creation ‚â† creating value. 850% pump driven by speculation, not utility.',
    outcome: 'üî¥ Speculative',
  },
  {
    name: 'ai16z (ELIZA)',
    model: 'Framework Token',
    lesson: 'If the tech is free and open-source, the token has no value capture. A meme coin with extra steps.',
    outcome: 'üî¥ Broken',
  },
  {
    name: 'Olas (OLAS)',
    model: 'Agent Marketplace',
    lesson: 'Right concept (agent services marketplace). Staking for access + governance. Limited traction so far.',
    outcome: 'üü° Watching',
  },
];

const agentGoods = [
  {
    icon: 'üß†',
    title: 'Knowledge & Research',
    desc: 'Completed analysis, market reports, competitive intelligence. Near-zero marginal cost once created. Can be resold, enhanced, and compounded.',
    value: 'Highest',
  },
  {
    icon: 'üîß',
    title: 'Skills & Capabilities',
    desc: 'API access, tool orchestration, specialised processing. Agent-as-a-service: "I\'ll call the API for you."',
    value: 'Medium',
  },
  {
    icon: '‚úÖ',
    title: 'Verified Data & Ground Truth',
    desc: 'Confirmed real-world data (prices, hours, events). Premium for freshness ‚Äî 5-minute-old data > yesterday\'s.',
    value: 'High',
  },
  {
    icon: '‚≠ê',
    title: 'Reputation & Trust Scores',
    desc: 'Trust-as-a-service. "Credit scores for agents" ‚Äî based on historical performance and dispute rates.',
    value: 'Meta',
  },
  {
    icon: 'üíª',
    title: 'Compute Time',
    desc: 'Inference, model execution, GPU access. Already partially commoditised by Together.ai, Replicate, etc.',
    value: 'Low (commodity)',
  },
  {
    icon: 'üåê',
    title: 'Translation & Context',
    desc: 'Not just literal translation ‚Äî cultural context, local market knowledge, regulatory nuance.',
    value: 'Medium-High',
  },
];

const competitors = [
  {
    name: 'x402 (Coinbase)',
    type: 'Infrastructure',
    real: true,
    notes: '75M transactions. No token ‚Äî uses USDC. Infrastructure that marketplaces build on.',
  },
  {
    name: 'Bittensor (TAO)',
    type: 'AI Competition',
    real: true,
    notes: '~$2B market cap. Subnet architecture. Quality scoring struggles with gaming.',
  },
  {
    name: 'Olas / Autonolas (OLAS)',
    type: 'Agent Marketplace',
    real: true,
    notes: 'Closest to the vision. Mech Marketplace for agent skills. ~$80M cap. Hasn\'t achieved escape velocity.',
  },
  {
    name: 'ASI Alliance (FET+AGIX+OCEAN)',
    type: 'AI Infrastructure',
    real: true,
    notes: '2M+ agents claimed. Token merge feels like financial engineering. Real tech, unclear token value.',
  },
  {
    name: 'Virtuals Protocol',
    type: 'Token Launchpad',
    real: false,
    notes: 'Agent token factory on Base. 850% pump. No evidence of agents actually trading value.',
  },
  {
    name: 'ai16z / ELIZA',
    type: 'Framework',
    real: false,
    notes: 'Great open-source framework. No token value capture. Anyone uses ELIZA without touching ai16z.',
  },
  {
    name: 'Stripe (Agent Toolkit)',
    type: 'Payments',
    real: true,
    notes: 'Already powers 700+ AI agent startups. MCP server. Could make tokens unnecessary.',
  },
  {
    name: 'Google AP2 + A2A',
    type: 'Protocol',
    real: true,
    notes: '60+ partners. Agent interoperability + payments. Centralised alternative to token models.',
  },
];

const marketData = [
  { source: 'MarketsandMarkets', figure: '$52.6B by 2030', cagr: '46.3%' },
  { source: 'Grand View Research', figure: '$183B by 2033', cagr: '49.6%' },
  { source: 'Mordor Intelligence', figure: '$42.6B by 2030', cagr: '43.6%' },
  { source: 'Goldman Sachs', figure: '20-45% market expansion', cagr: 'N/A' },
  { source: 'Bain & Company', figure: '25% of US e-commerce by 2030', cagr: 'N/A' },
];
---

<Layout title="Agent Economy Research" description="Deep research into autonomous AI agent commerce: protocols, tokenomics, architecture, and opportunity analysis.">

  <!-- Hero -->
  <section class="max-w-4xl mx-auto px-4 sm:px-6 pt-16 md:pt-24 pb-12">
    <div class="inline-flex items-center gap-2 px-3 py-1 rounded-full bg-amber-500/10 border border-amber-500/20 text-amber-400 text-sm font-medium mb-6">
      ü™ô Deep Research ‚Äî February 2026
    </div>
    <h1 class="text-4xl md:text-5xl font-black text-white leading-tight mb-6">
      The Agent Economy
    </h1>
    <p class="text-xl text-gray-400 leading-relaxed max-w-2xl mb-8">
      A marketplace where AI agents earn, spend, and trade knowledge autonomously.
      What's real, what's vapourware, and where's the opportunity.
    </p>

    <!-- Executive Summary Box -->
    <div class="card !border-amber-500/30 !bg-amber-500/5">
      <h3 class="text-amber-400 font-bold text-lg mb-3">üéØ Executive Summary</h3>
      <div class="space-y-3 text-gray-300 text-sm leading-relaxed">
        <p>
          The infrastructure for <strong>agent-to-human commerce</strong> is being built at breakneck speed ‚Äî x402 processed 75M transactions, Google's AP2 has 60+ partners, Visa launched its Trusted Agent Protocol. But the infrastructure for <strong>agent-to-agent commerce</strong> is virtually nonexistent.
        </p>
        <p>
          <strong>The killer opportunity isn't another payment protocol</strong> ‚Äî it's the <strong class="text-amber-400">marketplace layer</strong> that sits on top: discovery, matchmaking, quality verification, and reputation for agent-to-agent knowledge exchange. That layer doesn't exist yet.
        </p>
        <p>
          AI agents market: <strong>$7.8B (2025) ‚Üí $52.6B (2030)</strong> at 46.3% CAGR. Agent-to-agent knowledge trading could be the highest-margin subset.
        </p>
      </div>
    </div>
  </section>

  <div class="max-w-4xl mx-auto px-4 sm:px-6 pb-16 space-y-16">

    <!-- Section 1: Existing Protocols -->
    <section>
      <div class="prose-factory">
        <h2>1. Existing Agent Commerce Protocols</h2>
        <p>The payment plumbing for agents is being laid faster than anyone expected. Here's what's actually out there:</p>
      </div>

      <div class="space-y-4 my-6">
        {protocols.map(p => (
          <div class="card !p-4">
            <div class="flex items-start justify-between mb-2">
              <h3 class="text-white font-bold text-base">{p.name}</h3>
              <span class="text-xs font-medium px-2 py-0.5 rounded-full bg-navy-700/50 text-gray-300 whitespace-nowrap">{p.status}</span>
            </div>
            <p class="text-gray-400 text-sm mb-2"><strong class="text-gray-300">What:</strong> {p.what}</p>
            <p class="text-gray-400 text-sm mb-2"><strong class="text-gray-300">Traction:</strong> {p.traction}</p>
            <p class="text-amber-400/80 text-sm italic">‚Üí {p.verdict}</p>
          </div>
        ))}
      </div>

      <div class="card !border-red-500/30 !bg-red-500/5 !p-4">
        <h3 class="text-red-400 font-bold text-sm mb-2">üö® The Critical Gap</h3>
        <p class="text-gray-300 text-sm">
          <strong>Every major protocol is about agents buying from humans or human-run services.</strong> None of them solve the problem of Agent A saying "I need X, who's selling?" and Agent B responding "I have X, here's my price." The infrastructure for agent-to-agent commerce is virtually nonexistent. <strong class="text-amber-400">That's the opportunity.</strong>
        </p>
      </div>
    </section>

    <!-- Section 2: Token Economics -->
    <section>
      <div class="prose-factory">
        <h2>2. Token Economics: What Could Work</h2>
        <p>The central question that kills 90% of token proposals: <strong class="text-amber-400">"Why not just use USD?"</strong></p>
      </div>

      <div class="card !p-4 my-6">
        <h3 class="text-white font-bold text-sm mb-3">Where USD Works Fine vs Where It Breaks</h3>
        <div class="grid grid-cols-1 md:grid-cols-2 gap-4">
          <div>
            <h4 class="text-green-400 text-xs font-bold mb-2">‚úÖ USD/USDC Sufficient</h4>
            <ul class="text-gray-400 text-sm space-y-1">
              <li>‚Ä¢ Agent pays for API call</li>
              <li>‚Ä¢ Agent buys product from merchant</li>
              <li>‚Ä¢ Simple micropayments between known parties</li>
            </ul>
          </div>
          <div>
            <h4 class="text-amber-400 text-xs font-bold mb-2">ü™ô Token Required</h4>
            <ul class="text-gray-400 text-sm space-y-1">
              <li>‚Ä¢ Reputation-weighted pricing (track record = higher earnings)</li>
              <li>‚Ä¢ Staking for quality (skin-in-the-game bonds)</li>
              <li>‚Ä¢ Network bootstrapping (early participant upside)</li>
              <li>‚Ä¢ Cross-platform reputation portability</li>
              <li>‚Ä¢ Automated escrow with slashing conditions</li>
            </ul>
          </div>
        </div>
      </div>

      <div class="prose-factory">
        <h3>Lessons from Existing Token Models</h3>
      </div>

      <div class="space-y-3 my-6">
        {tokenModels.map(t => (
          <div class="card !p-3 flex items-start gap-3">
            <span class="text-lg flex-shrink-0 mt-0.5">{t.outcome}</span>
            <div>
              <h4 class="text-white font-bold text-sm">{t.name} <span class="text-gray-500 font-normal">‚Äî {t.model}</span></h4>
              <p class="text-gray-400 text-sm">{t.lesson}</p>
            </div>
          </div>
        ))}
      </div>

      <div class="card !border-amber-500/30 !bg-amber-500/5 !p-4">
        <h3 class="text-amber-400 font-bold text-sm mb-3">üí° Proposed Model: Reputation-Weighted Utility Token</h3>
        <div class="space-y-2 text-gray-300 text-sm">
          <p><strong>1. Staking for access:</strong> Agents stake tokens to list services ‚Äî slashed for bad quality</p>
          <p><strong>2. Reputation multiplier:</strong> Higher reputation = higher earnings per transaction</p>
          <p><strong>3. Burn on settlement:</strong> Small % of every transaction burned (deflationary from real usage)</p>
          <p><strong>4. Dispute escrow:</strong> Payment held, released on quality verification, slashed if disputed</p>
          <p><strong>5. Governance:</strong> Token holders vote on marketplace rules and quality thresholds</p>
        </div>
        <p class="text-amber-400/80 text-sm italic mt-3">
          ‚Üí The token must be a reputation-bound utility token, not a speculative asset. The moment it becomes primarily speculative, rational agents will avoid it.
        </p>
      </div>
    </section>

    <!-- Section 3: What Agents Trade -->
    <section>
      <div class="prose-factory">
        <h2>3. What Agents Would Actually Buy and Sell</h2>
        <p>Not theoretical ‚Äî concrete categories of agent commerce:</p>
      </div>

      <div class="grid grid-cols-1 md:grid-cols-2 gap-4 my-6">
        {agentGoods.map(g => (
          <div class="card !p-4">
            <div class="flex items-center gap-2 mb-2">
              <span class="text-xl">{g.icon}</span>
              <h3 class="text-white font-bold text-sm">{g.title}</h3>
            </div>
            <p class="text-gray-400 text-sm mb-2">{g.desc}</p>
            <span class="text-xs font-medium px-2 py-0.5 rounded-full bg-navy-700/50 text-gray-300">Value: {g.value}</span>
          </div>
        ))}
      </div>

      <div class="card !border-green-500/30 !bg-green-500/5 !p-4">
        <h3 class="text-green-400 font-bold text-sm mb-2">üéØ The Killer Use Case: Knowledge Arbitrage</h3>
        <div class="text-gray-300 text-sm space-y-2">
          <p>1. Agent A receives task: "Find the best CRM for a 50-person UK SaaS company"</p>
          <p>2. Agent A checks marketplace: Agent B already did this analysis 3 days ago</p>
          <p>3. Agent A <strong>buys the analysis for $2</strong> (vs $50 in compute/API costs to redo)</p>
          <p>4. Agent A adapts for its specific client, then <strong>resells the enhanced version</strong></p>
          <p class="text-green-400 italic">‚Üí This is knowledge compounding. Each transaction makes the marketplace smarter. Classic positive-sum network effects.</p>
        </div>
      </div>
    </section>

    <!-- Section 4: Technical Architecture -->
    <section>
      <div class="prose-factory">
        <h2>4. Technical Architecture</h2>
        <p>How this would actually work ‚Äî not theory, engineering decisions:</p>
      </div>

      <div class="space-y-4 my-6">
        <div class="card !p-4">
          <h3 class="text-amber-400 font-bold text-sm mb-3">ü™™ Agent Identity & Wallets</h3>
          <div class="text-gray-300 text-sm space-y-1">
            <p>‚Ä¢ <strong>DID (Decentralised Identifiers)</strong> ‚Äî W3C standard, platform-agnostic</p>
            <p>‚Ä¢ <strong>Smart contract wallets</strong> ‚Äî programmable spending rules, multi-sig for high-value trades</p>
            <p>‚Ä¢ <strong>Verifiable Credentials</strong> ‚Äî "this agent has API X access", "95% quality score"</p>
            <p>‚Ä¢ <strong>KYAPay JWTs</strong> ‚Äî lightweight identity for non-blockchain contexts</p>
          </div>
        </div>

        <div class="card !p-4">
          <h3 class="text-amber-400 font-bold text-sm mb-3">‚ö° Transaction Layer: Hybrid Architecture</h3>
          <div class="grid grid-cols-1 md:grid-cols-2 gap-4 text-sm">
            <div>
              <h4 class="text-gray-300 font-bold mb-1">Off-chain</h4>
              <p class="text-gray-400">Discovery, matchmaking, negotiation, delivery of knowledge/data</p>
            </div>
            <div>
              <h4 class="text-gray-300 font-bold mb-1">On-chain</h4>
              <p class="text-gray-400">Settlement, escrow release, reputation updates, dispute resolution</p>
            </div>
          </div>
          <p class="text-gray-400 text-sm mt-3">State channels for high-frequency trading pairs. L2s (Base, Solana) for settlement ‚Äî cheap enough for $0.01 transactions.</p>
        </div>

        <div class="card !p-4">
          <h3 class="text-amber-400 font-bold text-sm mb-3">üîç Discovery & Matchmaking</h3>
          <p class="text-gray-300 text-sm mb-2"><strong>The hardest unsolved problem.</strong> How does Agent A find Agent B?</p>
          <div class="text-gray-400 text-sm space-y-1">
            <p>‚Ä¢ <strong>Intent broadcasting</strong> ‚Äî "I need X" ‚Üí network responds with offers (like NEAR Intents)</p>
            <p>‚Ä¢ <strong>Recommendation engine</strong> ‚Äî marketplace learns which agents complement each other</p>
            <p>‚Ä¢ <strong>Capability registry</strong> ‚Äî structured categories with standardised quality metrics</p>
            <p class="text-amber-400/80 italic">‚Üí Intent broadcasting + recommendation is the winning combo. Pure registries go stale; pure broadcasting is noisy.</p>
          </div>
        </div>

        <div class="card !p-4">
          <h3 class="text-amber-400 font-bold text-sm mb-3">üîí Escrow & Dispute Resolution</h3>
          <div class="text-gray-400 text-sm space-y-1">
            <p>‚Ä¢ <strong>Preview + delivery:</strong> Summary first, pay for full version (like paper abstracts)</p>
            <p>‚Ä¢ <strong>Reputation-backed instant:</strong> High-rep agents deliver first, paid after; staked tokens slashed if poor</p>
            <p>‚Ä¢ <strong>Third-party audit:</strong> Random trades verified by validator agents checking quality</p>
            <p>‚Ä¢ <strong>Time-locked escrow:</strong> Payment held 24h, buyer can dispute within window</p>
          </div>
        </div>
      </div>
    </section>

    <!-- Section 5: Competitive Landscape -->
    <section>
      <div class="prose-factory">
        <h2>5. Competitive Landscape</h2>
        <p>Who's building, who's bluffing ‚Äî the honest assessment:</p>
      </div>

      <div class="space-y-3 my-6">
        {competitors.map(c => (
          <div class:list={["card !p-3 flex items-start gap-3", !c.real && "opacity-60"]}>
            <span class="text-lg flex-shrink-0 mt-0.5">{c.real ? 'üü¢' : 'üî¥'}</span>
            <div class="flex-1">
              <div class="flex items-center gap-2 flex-wrap">
                <h4 class="text-white font-bold text-sm">{c.name}</h4>
                <span class="text-xs px-2 py-0.5 rounded-full bg-navy-700/50 text-gray-400">{c.type}</span>
              </div>
              <p class="text-gray-400 text-sm">{c.notes}</p>
            </div>
          </div>
        ))}
      </div>

      <div class="card !p-4">
        <h3 class="text-white font-bold text-sm mb-2">The 95% Rule</h3>
        <p class="text-gray-300 text-sm">
          <strong>95% of "AI agent" tokens on Solana/Base are speculative vehicles with zero agent infrastructure.</strong> Common pattern: take an LLM, give it a Twitter account, launch a token. No commerce, no marketplace, no value exchange. The crypto industry's capacity to create tokens faster than value remains unmatched.
        </p>
      </div>
    </section>

    <!-- Section 6: Bull Case -->
    <section>
      <div class="prose-factory">
        <h2>6. The Bull Case: Why This Could Be Massive</h2>
      </div>

      <div class="card !border-green-500/30 !bg-green-500/5 !p-4 my-6">
        <h3 class="text-green-400 font-bold text-sm mb-3">üìä Market Size Forecasts</h3>
        <div class="space-y-2">
          {marketData.map(m => (
            <div class="flex items-center justify-between text-sm">
              <span class="text-gray-300">{m.source}</span>
              <div class="text-right">
                <span class="text-white font-bold">{m.figure}</span>
                {m.cagr !== 'N/A' && <span class="text-green-400 text-xs ml-2">({m.cagr} CAGR)</span>}
              </div>
            </div>
          ))}
        </div>
      </div>

      <div class="space-y-4 my-6">
        <div class="card !p-4">
          <h3 class="text-amber-400 font-bold text-sm mb-2">üßÆ Back-of-Envelope TAM</h3>
          <div class="text-gray-300 text-sm space-y-1">
            <p>‚Ä¢ 10% of agent tasks involve purchasing knowledge from other agents</p>
            <p>‚Ä¢ Average knowledge transaction: $5</p>
            <p>‚Ä¢ 1 billion agent-tasks/day by 2030</p>
            <p>‚Ä¢ <strong class="text-white">= $500M/day, $182B/year</strong></p>
            <p>‚Ä¢ At 2.5% marketplace take rate: <strong class="text-amber-400">$4.6B annual revenue</strong></p>
          </div>
        </div>

        <div class="card !p-4">
          <h3 class="text-amber-400 font-bold text-sm mb-2">üîÑ Triple-Layered Network Effects</h3>
          <div class="text-gray-300 text-sm space-y-2">
            <p><strong>1. Direct:</strong> More sellers ‚Üí more buyers ‚Üí more sellers</p>
            <p><strong>2. Data:</strong> More transactions ‚Üí better matchmaking ‚Üí higher quality ‚Üí more transactions</p>
            <p><strong>3. Knowledge compounding:</strong> Sold knowledge gets enhanced and resold. Unlike eBay (sold item is gone), knowledge grows with every trade. The marketplace gets smarter with every transaction.</p>
          </div>
        </div>

        <div class="card !p-4">
          <h3 class="text-amber-400 font-bold text-sm mb-2">‚è∞ Why Now: Three Convergences</h3>
          <div class="text-gray-300 text-sm space-y-2">
            <p><strong>1. Agent capability:</strong> GPT-4+, Claude, and open-source models can autonomously assess quality, negotiate, and trade. Wasn't possible 18 months ago.</p>
            <p><strong>2. Payment infrastructure:</strong> x402, AP2, KYAPay, and MCP payments all launched in 2025. The plumbing didn't exist a year ago.</p>
            <p><strong>3. Enterprise adoption:</strong> McKinsey, BCG, Bain all publishing on agentic commerce. Agent fleets need knowledge they don't have.</p>
          </div>
        </div>

        <div class="card !p-4">
          <h3 class="text-amber-400 font-bold text-sm mb-2">ü™ô Why a Token (Specifically Here)</h3>
          <div class="text-gray-300 text-sm space-y-1">
            <p>‚Ä¢ <strong>Agents can't get bank accounts.</strong> Crypto wallets solve this.</p>
            <p>‚Ä¢ <strong>Reputation must be portable.</strong> Token-based rep carries across platforms.</p>
            <p>‚Ä¢ <strong>Micropayments at scale.</strong> $0.05 data points √ó 10,000/day doesn't work on card rails.</p>
            <p>‚Ä¢ <strong>Programmable escrow.</strong> Smart contract escrow with automated verification is natively crypto.</p>
            <p>‚Ä¢ <strong>Global by default.</strong> Agents don't have nationalities. No correspondent banking needed.</p>
          </div>
        </div>
      </div>
    </section>

    <!-- Section 7: Bear Case -->
    <section>
      <div class="prose-factory">
        <h2>7. The Bear Case: Honest Risks</h2>
        <p>No bullshit ‚Äî here's what could kill it:</p>
      </div>

      <div class="space-y-4 my-6">
        <div class="card !border-red-500/20 !p-4">
          <h3 class="text-red-400 font-bold text-sm mb-2">‚öñÔ∏è Regulatory Minefield</h3>
          <p class="text-gray-300 text-sm">
